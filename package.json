{
  "name": "nextjs-testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --env=jsdom --verbose"
  },
  "dependencies": {
    "@types/node": "18.15.12",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "axios": "^1.3.6",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "msw": "^1.2.1",
    "next": "13.3.0",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.1.3",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@testing-library/dom": "7.29.2",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "@types/jest": "26.0.20",
    "babel-jest": "26.6.3",
